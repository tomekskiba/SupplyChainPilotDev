<% content_for :javascript do %>
    <%= render 'orders/header_includes' %>
<% end %>


<br/><br/>
<% title "Edit Order" %>
<h2 class="center">Edit Order</h2>

<%= form_for @order do |f| %>
    <%#= f.error_messages %>
    <%= render 'shared/error_messages', :object => f.object %>
    <%#= devise_error_messages! %>

    <%= render :partial => 'form', :locals => {:f => f} %>


    <p class="clear"></p>
    <br/><br/>
    <p>
      <%= f.submit "Continue", :class => "button_blue" %>
    </p>
<% end %>