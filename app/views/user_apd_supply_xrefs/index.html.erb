<% title "User Apd Supply Xrefs" %>

<table>
  <tr>
    <th>User</th>
    <th>Apd Supply</th>
    <th>Line Max</th>
    <th>Units Per Cycle</th>
  </tr>
  <% for user_apd_supply_xref in @user_apd_supply_xrefs %>
    <tr>
      <td><%= user_apd_supply_xref.user_id %></td>
      <td><%= user_apd_supply_xref.apd_supply_id %></td>
      <td><%= user_apd_supply_xref.line_max %></td>
      <td><%= user_apd_supply_xref.units_per_cycle %></td>
      <td><%= link_to "Show", user_apd_supply_xref %></td>
      <td><%= link_to "Edit", edit_user_apd_supply_xref_path(user_apd_supply_xref) %></td>
      <td><%= link_to "Destroy", user_apd_supply_xref, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New User Apd Supply Xref", new_user_apd_supply_xref_path %></p>
